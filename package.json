{
  "name": "react-state-cli",
  "version": "1.0.1",
  "description": "ðŸ§  Effortless Redux boilerplate generation from initial state files - Generate Redux Toolkit slices, reducers, and TypeScript interfaces automatically",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test:manual": "npm run build && node dist/index.js generate ./examples/initState.ts --verbose",
    "test:dry": "npm run build && node dist/index.js generate ./examples/initState.ts --dry-run"
  },
  "keywords": [
    "redux",
    "redux-toolkit",
    "cli",
    "boilerplate",
    "generator",
    "typescript",
    "react",
    "state-management",
    "automation",
    "slice",
    "reducer",
    "rtk"
  ],
  "author": "Utsav Virani",
  "license": "MIT",
  "homepage": "https://github.com/Utsav-Virani/react-state-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Utsav-Virani/react-state-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Utsav-Virani/react-state-cli/issues"
  },
  "type": "module",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "ejs": "^3.1.10",
    "fs-extra": "^11.3.0",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0"
  },
  "bin": {
    "react-state-cli": "./dist/index.js"
  }
}